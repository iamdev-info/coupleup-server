{
  "extends": "tslint-config-airbnb",
  "jsRules": {
    "curly": true
  },
  "rules": {
    "curly": true,
    "no-console": false,
    "member-access": false,
    "import-name": false,
    "object-shorthand-properties-first": false,
    "trailing-comma": false,
    "no-boolean-literal-compare": false,
    "variable-name": [true, "ban-keywords", "check-format", "allow-pascal-case"]
  }
}
